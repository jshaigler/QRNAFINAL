{
  "name": "rnafold-quantum-wasm",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && npm run copy-python",
    "copy-python": "node -e \"const fs=require('fs'),path=require('path');const src='src/python-modules',dest='dist/python-modules';fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src).forEach(f=>fs.copyFileSync(path.join(src,f),path.join(dest,f)))\"",
    "prepare": "npm run build"
  },
  "keywords": ["rna", "quantum", "folding", "bioinformatics", "webassembly", "rna-folding", "structure-prediction"],
  "author": "",
  "license": "ISC",
  "description": "Standalone TypeScript/JavaScript client for Quantum-Inspired RNA Folding using WebAssembly (Pyodide). No server required!",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/RnaFoldQuantum.git"
  },
  "dependencies": {
    "pyodide": "^0.26.4"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "@types/node": "^20.0.0"
  }
}
